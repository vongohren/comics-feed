{
  "name": "comics-feed",
  "version": "1.5.0",
  "description": "This is a server that parses pages with comics and serve them up as an rss feed",
  "private": true,
  "engines": {
    "node": "12.18.0"
  },
  "scripts": {
    "heroku-prebuild": "npm install --only=dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node lib/index.js",
    "dev": "babel-watch --inspect src/index.js",
    "build": "BABEL_ENV=commonjs babel src -d lib --copy-files"
  },
  "keywords": [
    "rss",
    "parser",
    "feed",
    "comics"
  ],
  "author": "Snorrre Lothar von Gohren Edwin <snorre.edwin@gmail.com> (https://github.com/Snorlock)",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^6.4.1",
    "@slack/client": "^3.9.0",
    "agenda": "^0.9.1",
    "axios": "^0.27.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cheerio": "^0.20.0",
    "cron": "^1.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "feed": "^0.3.0",
    "got": "^11.8.2",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.11.5",
    "node-uuid": "^1.4.7",
    "perfy": "^1.1.5",
    "request": "^2.71.0",
    "request-promise": "^4.2.1",
    "slack-node": "^0.1.8",
    "winston": "^2.3.1",
    "winston-papertrail": "^1.0.4",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babel-watch": "^2.0.6",
    "mongoui": "^4.1.2"
  },
  "heroku-run-build-script": true
}
